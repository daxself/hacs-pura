{
  "entity": {
    "select": {
      "fragrance": {
        "default": "mdi:scent",
        "state": {
          "off": "mdi:scent-off"
        }
      },
      "intensity": {
        "default": "mdi:signal-cellular-outline",
        "state": {
          "subtle": "mdi:signal-cellular-1",
          "medium": "mdi:signal-cellular-2",
          "strong": "mdi:signal-cellular-3"
        }
      }
    },
    "sensor": {
      "controller": {
        "default": "mdi:toggle-switch-outline",
        "state": {
          "away": "mdi:map-marker-radius-outline",
          "schedule": "mdi:calendar-blank-outline",
          "timer": "mdi:timer-outline"
        }
      }
    }
  },
  "services": {
    "start_timer": {
      "service": "mdi:timer"
    }
  }
}